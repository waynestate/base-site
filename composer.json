{
    "name": "base",
    "description": "Wayne State University Base Laravel Site",
    "keywords": [
        "framework",
        "laravel",
        "base"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-debugbar": "^3.1",
        "fideloper/proxy": "~4.0",
        "fzaninotto/faker": "~1.4",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1",
        "waynestate/apdatetime": "^1.0",
        "waynestate/display-menu": "~1.0",
        "waynestate/error-403": "~1.1",
        "waynestate/error-404": "~1.1",
        "waynestate/error-500": "~1.1",
        "waynestate/formy-parser": "~1.0",
        "waynestate/parse-menu": "~1.2",
        "waynestate/parse-promos": "~2.0",
        "waynestate/waynestate-api": "~1.2",
        "waynestate/image-faker": "~1.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.2",
        "laravel/envoy": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~7.0",
        "php-coveralls/php-coveralls": "^2.0",
        "nunomaduro/collision": "^2.0"
    },
    "autoload": {
        "files": [
            "app/Support/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Factories\\": "factories/",
            "Styleguide\\": "styleguide/",
            "Contracts\\": "contracts"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1.3"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
